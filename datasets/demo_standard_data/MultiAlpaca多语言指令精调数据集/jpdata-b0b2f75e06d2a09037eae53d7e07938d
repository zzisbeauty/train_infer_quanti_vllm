import os
path = ""/Users/user/documents/""
merger = PdfFileMerger()
for filename in os.listdir(path):
    if filename.endswith("".pdf""):
        file_path = os.path.join(path, filename)
        merger.append(file_path)
merger.write(""merged.pdf"")"
